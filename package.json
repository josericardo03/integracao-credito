{
  "name": "integracao-credito",
  "version": "1.0.0",
  "description": "Classe HTTP Client para requisições",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npm run build && node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "example": "ts-node examples/usage.ts",
    "example:build": "npm run build && node dist/examples/usage.js",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "http",
    "client",
    "request",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0 || >=20.0.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
